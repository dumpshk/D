#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<int> price = {0, 2, 5, 7, 8}; 
    int n = 4;
    vector<int> dp(n + 1, 0);

    for (int i = 1; i <= n; i++) {
        int mx = INT_MIN;
        for (int j = 1; j <= i; j++) {
            mx = max(mx, price[j] + dp[i - j]);
        }
        dp[i] = mx;
    }

    cout << "Maximum value: " << dp[n];
    return 0;
}
